<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <!-- Background -->
  <rect width="800" height="600" fill="#f8f9fa"/>
  
  <!-- Corporate uniform illustration -->
  <g transform="translate(400, 300)">
    <!-- Shirt -->
    <path d="M-120,-150 L120,-150 L140,-50 L140,150 L-140,150 L-140,-50 Z" fill="#ffffff" stroke="#333" stroke-width="3"/>
    
    <!-- Collar -->
    <path d="M-60,-150 L-40,-180 L0,-150 L40,-180 L60,-150 L0,-120 Z" fill="#ffffff" stroke="#333" stroke-width="2"/>
    
    <!-- Tie -->
    <path d="M-5,-150 L5,-150 L15,-100 L0,150 L-15,-100 Z" fill="#dc3545" stroke="#333" stroke-width="2"/>
    
    <!-- Company logo -->
    <circle cx="-60" cy="-50" r="30" fill="#0d6efd" stroke="#333" stroke-width="2"/>
    <text x="-60" y="-45" font-family="Arial, sans-serif" font-size="12" font-weight="bold" text-anchor="middle" fill="#ffffff">STC</text>
    
    <!-- Pocket -->
    <rect x="40" y="-80" width="40" height="50" rx="5" fill="#ffffff" stroke="#333" stroke-width="2"/>
    
    <!-- Buttons -->
    <circle cx="0" cy="-80" r="5" fill="#333"/>
    <circle cx="0" cy="-30" r="5" fill="#333"/>
    <circle cx="0" cy="20" r="5" fill="#333"/>
    <circle cx="0" cy="70" r="5" fill="#333"/>
    
    <!-- Sleeves -->
    <path d="M-120,-150 L-200,-80 L-180,-60 L-140,-50" fill="#ffffff" stroke="#333" stroke-width="3"/>
    <path d="M120,-150 L200,-80 L180,-60 L140,-50" fill="#ffffff" stroke="#333" stroke-width="3"/>
  </g>
  
  <!-- Text -->
  <text x="400" y="550" font-family="Arial, sans-serif" font-size="30" font-weight="bold" text-anchor="middle" fill="#333">زي الشركات</text>
</svg>